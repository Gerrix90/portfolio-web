{
  "name": "ouq77-portfolio",
  "version": "2.14.1",
  "description": "On-line portfolio of Louw Swart, based on 'angular2-seed'",
  "engines": {
    "node": "7.x.x",
    "npm": "3.x.x"
  },
  "repository": {
    "url": "https://github.com/ouq77/portfolio-web"
  },
  "scripts": {
    "build.dev": "gulp build.dev",
    "build.dev.watch": "gulp build.dev.watch",
    "build.prod": "gulp build.prod --env prod",
    "clean-ng2": "rimraf node_modules/angular2/manual_typings node_modules/angular2/typings",
    "gulp": "gulp",
    "lint": "gulp tslint",
    "postinstall": "typings install && npm prune",
    "reinstall": "npm cache clean && rimraf typings/* && rimraf node_modules/* && npm install",
    "start": "gulp serve.dev",
    "serve.dev": "gulp serve.dev",
    "serve.prod": "gulp serve.prod",
    "tasks.list": "gulp --tasks-simple"
  },
